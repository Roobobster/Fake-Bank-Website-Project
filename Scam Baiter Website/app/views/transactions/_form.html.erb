<table id="transaction_form">
    <tr>
        <th>Account ID</th>
        <td><%= f.text_field(:account_id, :id => "account_id") %></td>
    </tr>
    <tr>
        <th>Date</th>
        <td><%= f.text_field(:transaction_datetime, :id => "transaction_datetime") %></td>
    </tr>
    <tr>
        <th>Transactor</th>
        <td><%= f.text_field(:transactor, :id => "transactor") %></td>
    </tr>
    <tr>
        <th>Message</th>
        <td><%= f.text_field(:message) %></td>
    </tr>
    <tr>
        <th>Amount</th>
        <td><%= f.text_field(:amount) %></td>
    </tr>
</table>