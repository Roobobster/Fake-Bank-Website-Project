<table id="transaction_form">
    <tr>
        <th>Account ID</th>
        <td><%= @transaction.account_id %></td>
    </tr>
    <% if @transaction.id %>
        <tr>
            <th>Reference</th>
            <td><%= @transaction.id %></td>
        </tr>
    <% end %>
    <tr>
        <th>Date</th>
        <td><%= f.text_field(:transaction_datetime) %></td>
    </tr>
    <tr>
        <th>Transactor</th>
        <td><%= f.text_field(:transactor) %></td>
    </tr>
    <tr>
        <th>Message</th>
        <td><%= f.text_field(:message) %></td>
    </tr>
    <tr>
        <th>Amount</th>
        <td><%= f.text_field(:amount) %></td>
    </tr>
</table>