#   </tr>

<tr>
    <th>Address</th>
    <td><%= @users.address %></td>
</tr>


<tr>
    <th>Phone Number</th>
    <td><%= @users.telephone %></td>
</tr>


<tr>
    <th>First Name</th>
    <td><%= @users.first_name %></td>
</tr>


<tr>
    <th>Last Name</th>
    <td><%= @users.last_name %></td>
</tr>

<tr>
    <th>Age</th>
