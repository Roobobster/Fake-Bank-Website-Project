

<div class="admin show">
  <h2>Show Profile</h2>

  <table summary="Profile detail view">
    <tr>
      <th>Username</th>
      <td><%= @users.username %></td>
    </tr>

    <tr>
        <th>Email</th>
        <td><%= @users.email %></td>
    </tr>

#   <tr>
#         <th>Password</th>
